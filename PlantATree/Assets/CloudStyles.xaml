<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--######################################CLOUD#######################################-->
    <!--Cloud gradient-->
    <LinearGradientBrush x:Key="CloudGradientBrush" StartPoint="0.515235,0.0502294" EndPoint="0.515235,0.968037">
        <GradientStop Color="#FFFEFEFE" Offset="0"/>
        <GradientStop Color="#FFE8F8FD" Offset="0.370455"/>
        <GradientStop Color="#FFD2F2FD" Offset="0.674419"/>
        <GradientStop Color="#FFE8F8FE" Offset="0.803759"/>
        <GradientStop Color="#FFFFFFFF" Offset="1"/>
    </LinearGradientBrush>

    <!--Cloud Stroke-->
    <SolidColorBrush x:Key="CloudStroke" Color="#FF95D4FA"/>

    <!--Cloud Path Style-->
    <Style x:Key="PathStyleCloud" TargetType="Path">
        <Setter Property="MinWidth" Value="24"/>
        <Setter Property="MinHeight" Value="48"/>
        <Setter Property="Stretch" Value="Fill"/>
        <Setter Property="StrokeLineJoin" Value="Round"/>
        <Setter Property="Stroke" Value="{StaticResource CloudStroke}"/>
        <Setter Property="Fill" Value="{StaticResource  CloudGradientBrush}" />
		<Setter Property="UseLayoutRounding" Value="False"/>
    </Style>

    
</ResourceDictionary>
