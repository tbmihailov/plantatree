            </TransformGroup>
        </LinearGradientBrush.RelativeTransform>
        <GradientStop Color="#FF62C121" Offset="0"/>
        <GradientStop Color="#FF4DA213" Offset="0.511628"/>
        <GradientStop Color="#FF388306" Offset="1"/>
    </LinearGradientBrush>

    <Style x:Key="TreeCrownMiddlePartPathStyle" TargetType="Path">
        <Setter Property="Stretch" Value="Fill"/>
