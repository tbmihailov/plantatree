    </Style>


    <!--Tree Crown Front Part 1-->
    <LinearGradientBrush x:Key="TreeCrownFrontPartOneFillBrush" StartPoint="0.466464,0.0247227" EndPoint="1.30555,0.0247227">
        <LinearGradientBrush.RelativeTransform>
            <TransformGroup>
                <SkewTransform CenterX="0.466464" CenterY="0.0247227" AngleX="3.06972" AngleY="0"/>
                <RotateTransform CenterX="0.466464" CenterY="0.0247227" Angle="87.5509"/>
            </TransformGroup>
        </LinearGradientBrush.RelativeTransform>
        <GradientStop Color="#FF54AD18" Offset="0"/>
        <GradientStop Color="#FF47990F" Offset="0.511628"/>
        <GradientStop Color="#FF3A8607" Offset="1"/>
