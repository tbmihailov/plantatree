    <!--Tree Crown Front Part 2-->
    <LinearGradientBrush x:Key="TreeCrownFrontPartTwoFillBrush" StartPoint="0.466464,0.0247231" EndPoint="1.30555,0.0247231">
        <LinearGradientBrush.RelativeTransform>
            <TransformGroup>
                <SkewTransform CenterX="0.466464" CenterY="0.0247231" AngleX="3.06974" AngleY="0"/>
                <RotateTransform CenterX="0.466464" CenterY="0.0247231" Angle="87.5509"/>
            </TransformGroup>
        </LinearGradientBrush.RelativeTransform>
        <GradientStop Color="#FF58B31B" Offset="0"/>
        <GradientStop Color="#FF499C11" Offset="0.511628"/>
